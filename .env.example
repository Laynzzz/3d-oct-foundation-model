# Backblaze B2 Storage Configuration (S3-compatible)
# Copy to .env and fill in your credentials
AWS_ACCESS_KEY_ID="your_b2_key_id_here"
AWS_SECRET_ACCESS_KEY="your_b2_application_key_here"
AWS_DEFAULT_REGION="us-west-004"
S3_ENDPOINT_URL="https://s3.us-west-004.backblazeb2.com"

# Optional: Local cache directory for OCT data
DATA_CACHE_DIR="/tmp/oct_cache"

# W&B Configuration (optional)
WANDB_PROJECT="3d-oct-foundation-model"
WANDB_ENTITY="laynzzz-university-at-buffalo"